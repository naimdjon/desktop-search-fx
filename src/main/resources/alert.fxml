<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>
<HBox styleClass="alert-dialog"
      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="768.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/"
      fx:controller="org.desktopsearch.AlertDialogController">
    <children>
        <Label fx:id="messageLabel"/>
        <Button mnemonicParsing="false" text="OK" fx:id="okButton" cancelButton="true" onAction="#closeDialog"/>
    </children>
    <stylesheets>
        <URL value="@alert.css"/>
    </stylesheets>
</HBox>